{
  "framework": "vite",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/|robots\\.txt|sitemap\\.xml).*)",
      "destination": "/index.html"
    }
  ]
}
